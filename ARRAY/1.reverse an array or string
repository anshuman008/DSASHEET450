// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;
int main() {
  
  // reverse a array
  int arr[] = {1,2,3,4,5,6};
  int n = sizeof(arr)/sizeof(arr[0]);
  
  int s = 0;
  int e = n-1;
  while(s<e){
      // swap arr[s], arr[e]
      int temp = arr[s];
      arr[s] = arr[e];
      arr[e] = temp;
      
      s++;
      e--;
  }
  
  for(int i = 0; i<n; i++){
      cout<<arr[i]<<" ";
  }
  
 //  reverse a string
  
  string st = "anshuman";
  int s = 0;
  int e = st.size()-1;
  
      while(s<e){
      // swap arr[s], arr[e]
      char temp = st[s];
      st[s] = st[e];
      st[e] = temp;
      
      s++;
      e--;
  }
  
  cout<<st<<endl;
    return 0;
}
